<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:RplaceModtools.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RplaceModtools.Views.NotificationStateInfo">
    <Design.DataContext>
        <viewModels:NotificationStateInfoViewModel/>
    </Design.DataContext>
    
    <TextBlock Foreground="Yellow" Text="{Binding Notification}"></TextBlock>
</UserControl>
